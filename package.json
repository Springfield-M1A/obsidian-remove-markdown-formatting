{
    "name": "remove-markdown-formatting",
    "version": "0.1.0",
    "description": "Selectively remove specific Markdown syntax elements or custom phrases from selected text in Obsidian.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian-plugin",
        "markdown",
        "text-formatting",
        "custom-removal",
        "editor"
    ],
    "author": "Springfield-M1A",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.17.3",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    }
}